<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gamecollector" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <name>Hadougame</name>
    <description>
        Organize sua coleção de jogos de Super Nintendo de maneira fácil e rápida.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Hadougame Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon height="72" src="img/hadougame-logo.png" width="72" />
    <icon height="48" src="img/hadougame-logo.png" width="48" />
    <icon height="36" src="img/hadougame-logo.png" width="36" />
</widget>
